{
  "type": "project",
  "name": "name/foo-bundle",
  "description": "The place where you should describe your bundle",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.1.0",
    "symfony/console": "6.2.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.1",
    "symfony/browser-kit": "^6.2",
    "symfony/framework-bundle": "^6.2",
    "symfony/yaml": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "Eniams\\FooBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eniams\\FooBundle\\Tests\\": "tests/"
    }
  }
}